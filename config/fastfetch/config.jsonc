      "keyColor": "green"
    },
    {
      "type": "gpu",
      "key": "│ ├󰍛 GPU",
      "keyColor": "green"
    },
    {
      "type": "memory",
      "key": "│ ├󰍛 RAM",
      "keyColor": "green"
    },
    {
      "type": "swap",
      "key": "│ ├󰓡 Swap",
      "keyColor": "green"
    },
    {
      "type": "disk",
      "key": "└ └ Disk",
      "keyColor": "green"
    },
    {
      "type": "battery",
      "key": "   ├ Battery",
      "keyColor": "green"
