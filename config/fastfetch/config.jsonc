{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/logos/cstm_logo.ansi",
      "padding": {
        "top": 4,
        "left": 0,
        "right": 2
    },
  },
  "modules": [
