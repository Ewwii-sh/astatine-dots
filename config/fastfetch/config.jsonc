    {
      "type": "cursor",
      "key": "│ ├ Cursor",
      "keyColor": "blue"
    },
    {
      "type": "terminal",
      "key": "│ ├ Terminal",
      "keyColor": "blue"
    },
    {
      "type": "terminalfont",
      "key": "└ └ TermFont",
      "keyColor": "blue"
    },
    "break",
    {
      "type": "uptime",
      "key": "󰅐 Uptime",
      "keyColor": "magenta"
    },
    {
      "type": "locale",
      "key": " Locale",
      "keyColor": "magenta"
